<view><view class="tab" style="{{'top:'+(gauge+'rpx')+';'}}"><scroll-view class="tab_scroll" scroll-x="{{true}}" scroll-into-view="{{intoindex}}"><block wx:for="{{lists}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="{{[tab_index==index?'tab_scroll_list':'tab_scroll_lists']}}" style="{{'color:'+(tab_index==index?tab_color:'')+';'}}" id="{{'tab'+index}}" data-event-opts="{{[['tap',[['cut',[index,'$0'],[[['lists','',index,'id']]]]]]]}}" bindtap="__e">{{''+item.name+''}}</view></block><view class="tab_scroll_list"></view></scroll-view><block wx:if="{{lists.length>6}}"><view data-event-opts="{{[['tap',[['open',['$event']]]]]}}" class="pull" bindtap="__e"><image src="../static/img/pull.png"></image></view></block></view><view hidden="{{!(or)}}" class="popup"></view><view class="{{[or?'popup_list':'popup_lists']}}" style="{{'top:'+(gauge+100+'rpx')+';'}}"><view hidden="{{!(or_text)}}" class="label"><scroll-view class="label_scroll" scroll-y="{{true}}"><block wx:for="{{lists}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['cut',[index,'$0'],[[['lists','',index,'id']]]]]]]}}" class="label_list" style="{{'background:'+(tab_index==index?tab_color:'')+';'}}" bindtap="__e">{{''+item.name+''}}</view></block></scroll-view></view></view></view>