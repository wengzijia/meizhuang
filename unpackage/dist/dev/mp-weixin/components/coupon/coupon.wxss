.flex {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.Color {
  color: #ffdfbe;
}
.textCenter {
  text-align: center;
}
.couponTotalContainer {
  position: relative;
}
.couponTotalContainer .couponContainer {
  width: 94%;
  height: 200rpx;
  border: 1px solid red;
  background-color: #2b2e3d;
  border-radius: 20rpx;
  margin: 20rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.couponTotalContainer .couponContainer .moneyContainer {
  margin-left: 30rpx;
}
.couponTotalContainer .couponContainer .moneyContainer .money .sum {
  font-size: 60rpx;
}
.couponTotalContainer .couponContainer .moneyContainer .useTerm {
  font-size: 20rpx;
}
.couponTotalContainer .couponContainer .couponInfo {
  margin-left: 60rpx;
}
.couponTotalContainer .couponContainer .couponInfo .title {
  font-size: 36rpx;
}
.couponTotalContainer .couponContainer .couponInfo .indate {
  margin-top: 12rpx;
  font-size: 24rpx;
}
.couponTotalContainer .couponContainer .pastDue {
  position: absolute;
  top: 0;
  right: 24rpx;
  font-size: 100rpx;
  color: #dd524d;
  -webkit-transform: rotate(348deg);
          transform: rotate(348deg);
}

