.flex {
  display: flex;
  justify-content: space-between;
}
.flexC {
  display: flex;
  align-items: center;
}
.flex_l_r {
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.flex_c_c {
  display: flex;
  justify-content: center;
  align-items: center;
}
.center {
  text-align: center;
}
.fs {
  font-size: 26rpx;
}
.size {
  font-size: 24rpx;
}
.border:after {
  display: block;
  content: "";
  width: 40rpx;
  margin-top: 12rpx;
  margin-left: 12rpx;
  border-bottom: 8rpx solid #000000;
  border-radius: 20rpx;
}
.particularsWrap {
  padding: 10rpx;
  text-align: center;
  display: flex;
  align-items: center;
  position: fixed;
  top: 0;
  width: 100%;
  background-color: #FFFFFF;
  padding-top: 80rpx;
  z-index: 9999;
}
.particularsWrap .particulars {
  width: 100%;
  line-height: 60rpx;
}
.particularsWrap .navigationInfo {
  margin: auto;
}
.particularsWrap .navigationInfo .item {
  margin: 0 20rpx;
}
.goodsDetailContainer {
  background-color: #f7f7f7;
  margin-top: 140rpx;
}
.goodsDetailContainer .header {
  background-color: #FFFFFF;
  height: 1200rpx;
}
.goodsDetailContainer .header .multicastDiagramContainer .multicastDiagram {
  height: 600rpx;
}
.goodsDetailContainer .header .multicastDiagramContainer .multicastDiagram .item image {
  width: 100%;
  height: 100%;
}
.goodsDetailContainer .header .multicastDiagramContainer .titleContainer {
  padding: 30rpx;
  height: 120rpx;
}
.goodsDetailContainer .header .multicastDiagramContainer .goodsInfo {
  padding: 30rpx;
}
.goodsDetailContainer .header .multicastDiagramContainer .goodsInfo .priceWrap {
  color: #dd524d;
  font-size: 36rpx;
}
.goodsDetailContainer .header .multicastDiagramContainer .goodsInfo .priceWrap .bigNumber {
  font-size: 50rpx;
}
.goodsDetailContainer .header .multicastDiagramContainer .goodsInfo .soldOut {
  margin-left: 20rpx;
  background-color: #b2b2b2;
  color: #FFFFFF;
  border-radius: 20rpx;
  text-align: center;
  width: 160rpx;
  height: 40rpx;
}
.goodsDetailContainer .header .multicastDiagramContainer .goodsInfo .getCouponWrap .greaterThan {
  margin-left: 12rpx;
}
.goodsDetailContainer .header .multicastDiagramContainer .popup .receiveCoupon {
  height: 800rpx;
  background-color: #FFFFFF;
  border-top-left-radius: 40rpx;
  border-top-right-radius: 40rpx;
}
.goodsDetailContainer .header .multicastDiagramContainer .popup .receiveCoupon .title {
  padding-top: 20rpx;
}
.goodsDetailContainer .header .multicastDiagramContainer .popup .receiveCoupon .discountCoupon {
  margin-top: 100rpx;
}
.goodsDetailContainer .header .multicastDiagramContainer .safeguard {
  margin: 20rpx;
  margin-top: 40rpx;
  border: 2rpx solid #eeeeee;
  height: 60rpx;
  padding: 20rpx;
}
.goodsDetailContainer .header .multicastDiagramContainer .specification {
  margin: 60rpx 30rpx;
}
.goodsDetailContainer .commentContainer {
  background-color: #FFFFFF;
  padding: 30rpx;
  margin: 30rpx 0rpx;
}
.goodsDetailContainer .commentContainer .commentInfo {
  padding: 30rpx 0rpx;
}
.goodsDetailContainer .commentContainer .commentInfo .praise {
  display: flex;
  align-items: center;
}
.goodsDetailContainer .commentContainer .commentInfo .praise .vessel .imgWrap {
  width: 80rpx;
  height: 80rpx;
}
.goodsDetailContainer .commentContainer .commentInfo .praise .vessel .imgWrap .headPortrait {
  width: 100%;
  height: 100%;
  border-radius: 50%;
}
.goodsDetailContainer .commentContainer .commentInfo .praise .vessel .nickname {
  margin-left: 30rpx;
}
.goodsDetailContainer .commentContainer .commentInfo .praise .vessel .nickname .name {
  color: #444444;
}
.goodsDetailContainer .commentContainer .commentInfo .praise .vessel .nickname .dateTime {
  font-size: 20rpx;
  color: #999999;
}
.goodsDetailContainer .commentContainer .commentInfo .commentWrap .comment {
  margin: 30rpx 0rpx;
  color: #666666;
}
.goodsDetailContainer .commentContainer .commentInfo .commentWrap .picturesContainer {
  width: 200rpx;
  height: 200rpx;
}
.goodsDetailContainer .commentContainer .commentInfo .commentWrap .picturesContainer .pictures {
  width: 100%;
  height: 100%;
}
.goodsDetailContainer .detailContainer {
  margin-bottom: 180rpx;
  background-color: #FFFFFF;
}
.goodsDetailContainer .detailContainer .goodsDetail {
  padding: 30rpx;
}
.goodsNavigation {
  background-color: #FFFFFF;
  position: fixed;
  bottom: 0;
  left: 0;
  right: 0;
  padding: 30rpx;
}
.goodsNavigation .serviceWrap .service {
  text-align: center;
}
.goodsNavigation .serviceWrap .service .serviceIcon {
  font-size: 40rpx;
}
.goodsNavigation .serviceWrap .service .serviceTitle {
  font-size: 20rpx;
}
.goodsNavigation .serviceWrap .shopping {
  margin-left: 60rpx;
  text-align: center;
}
.goodsNavigation .serviceWrap .shopping .cartIcon {
  font-size: 40rpx;
}
.goodsNavigation .serviceWrap .shopping .cartTitle {
  font-size: 20rpx;
}
.goodsNavigation .buyContainer {
  margin-left: 80rpx;
}
.goodsNavigation .buyContainer .cart {
  border: 1rpx solid #cccccc;
  border-radius: 6rpx;
  background-color: #ffffff;
  font-size: 20rpx;
  height: 80rpx;
  width: 200rpx;
}
.goodsNavigation .buyContainer .buy {
  margin-left: 30rpx;
  border: 1rpx solid #cccccc;
  border-radius: 6rpx;
  background-color: #2b2e3d;
  color: #FFFFFF;
  font-size: 20rpx;
  height: 80rpx;
  width: 200rpx;
}

